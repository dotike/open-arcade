{
   "service": "nginxaarf",
   "description": "nginxaarf service",
   "version": 4,
   "component_type": "k8s",
   "containers": [
      {
         "name": "aarf",
         "image": "nginx:latest",
         "readiness_check_path": "/",
         "readiness_check_port": 80,
         "cpu": 1,
         "cpu_limit": 2,
         "mem": 1024,
         "mem_limit": 2048,
         "port_mappings": [
            {
              "port": 80,
              "port_name": "http"
            }
         ]
      }
   ],
   "service_options": {
      "alerting": true,
      "desired_count": 1,
      "load_balanced": {
          "public": true,
          "private": true
      }
   },
   "application_config": {
      "config_val_1": "foo",
      "config_val_2": "bar",
      "config_val_3": "baz"
   },
   "tags": {
      "test_key": "test_value",
      "test_key2": "test_value_two"
   },
   "metadata": {
        "Service Name": "nginxaarf",
        "Description": "A demo service to show how Arcade tooling works",
        "Owner": "IP TOOLS"
    }
}
